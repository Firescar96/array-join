{
  "name": "array-join",
  "version": "2.3.0",
  "description": "Join (SQL-like) arrays of objects by a common key or with a custom match function.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iredchuk/array-join"
  },
  "main": "src/index.js",
  "author": {
    "name": "Igor Redchuk",
    "email": "redchuk@gmail.com"
  },
  "keywords": [
    "array",
    "arrays",
    "join",
    "left-join",
    "util"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "lint": "standard",
    "prepare": "npm test"
  },
  "dependencies": {},
  "devDependencies": {
    "jest": "^21.0.2",
    "standard": "^10.0.3",
    "babel-core": "^6.25.0",
    "babel-plugin-add-module-exports": "~0.2",
    "babel-preset-es2015": "~6.24",
    "chance": "~1.0",
    "gulp": "~3.9",
    "gulp-benchmark": "~1.1",
    "gulp-load-plugins": "~1.5"
  },
  "standard": {
    "env": {
      "jest": true
    }
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
